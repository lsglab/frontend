{
	"name": "musin-frontend",
	"version": "0.0.1",
	"private": true,
	"description": "This is our new school website frontend",
	"license": "AGPL-3.0-only",
	"scripts": {
		"do:build": "run-s do:checkup run:build",
		"do:checkup": "run-s format lint test",
		"do:commit": "yarn git:commit",
		"do:cypress": "cypress open",
		"do:dev": "run-s format run:hot",
		"do:export": "run-s do:checkup run:export",
		"do:serve": "run-s do:checkup run:serve",
		"do:start": "yarn run:start",
		"do:sync": "yarn git:sync",
		"eslint:fix": "eslint --fix --cache .",
		"eslint:report": "eslint --cache .",
		"format": "run-s format:*",
		"format:code": "run-s format:code:*",
		"format:code:prettier": "yarn prettier:fix",
		"git:add": "git add -A",
		"git:commit": "run-s git:commit:*",
		"git:commit:add": "yarn git:add",
		"git:commit:checks": "yarn do:checkup",
		"git:commit:do": "cz",
		"git:sync": "yarn warn:soydows && ./Scripts/sync.sh",
		"lint": "run-p --race lint:*",
		"lint:scripts": "run-p --race lint:scripts:*",
		"lint:scripts:eslint": "yarn eslint:fix",
		"lint:styles": "run-p --race lint:styles:*",
		"lint:styles:stylelint": "yarn stylelint:fix",
		"lint:styles:validate": "yarn stylelint:validate",
		"prettier:fix": "prettier --write .",
		"prettier:report": "prettier --check .",
		"run:build": "cross-env NODE_ENV=production run-s run:build:*",
		"run:build:do": "sapper build --legacy",
		"run:export": "cross-env NODE_ENV=production run-s run:export:*",
		"run:export:do": "sapper export --legacy",
		"run:hot": "cross-env NODE_ENV=development run-s run:hot:*",
		"run:hot:do": "sapper dev",
		"run:serve": "run-s run:build run:start",
		"run:start": "cross-env NODE_ENV=production run-s run:start:*",
		"run:start:do": "node __sapper__/build",
		"stylelint:fix": "stylelint . --fix --cache",
		"stylelint:report": "stylelint . --cache",
		"stylelint:validate": "stylelint-config-prettier-check",
		"test": "run-p --race run:serve test:*",
		"test:cypress": "cypress run",
		"warn:soydows": "echo 'Warning: This will fail if you're on a non-unix-like operating system, or more specifically, Windows. Get WSL, or just install Linux directly.'"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"cross-env": "^7.0.2",
		"polka": "next",
		"sirv": "^1.0.6"
	},
	"devDependencies": {
		"@babel/core": "^7.11.4",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/runtime-corejs3": "^7.11.2",
		"@commitlint/cli": "^9.1.2",
		"@commitlint/config-conventional": "^9.1.2",
		"@rollup/plugin-babel": "^5.2.0",
		"@rollup/plugin-commonjs": "^15.0.0",
		"@rollup/plugin-image": "^2.0.5",
		"@rollup/plugin-legacy": "^2.0.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-strip": "^2.0.0",
		"@rollup/plugin-url": "^5.0.1",
		"autoprefixer": "^9.8.6",
		"babel-eslint": "^10.1.0",
		"colorguard": "^1.2.1",
		"commitizen": "^4.2.1",
		"core-js": "^3.6.5",
		"cssnano": "^4.1.10",
		"cssnano-preset-advanced": "^4.0.7",
		"cypress": "^5.0.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.7.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-cypress": "^2.11.1",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-svelte3": "^2.7.3",
		"fibers": "^5.0.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.13",
		"npm-run-all": "^4.1.5",
		"postcss-assets": "^5.0.0",
		"postcss-autoreset": "^2.0.2",
		"postcss-fail-on-warn": "^0.1.0",
		"postcss-font-magician": "^2.3.1",
		"postcss-import": "^12.0.1",
		"postcss-initial": "^3.0.2",
		"postcss-sprites": "^4.2.1",
		"postcss-url": "^8.0.0",
		"prettier": "^2.1.1",
		"prettier-plugin-package": "^1.0.0",
		"prettier-plugin-svelte": "^1.1.1",
		"rollup": "^2.26.6",
		"rollup-plugin-minify-html-literals": "^1.2.5",
		"rollup-plugin-sass": "^1.2.2",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"sapper": "^0.28.2",
		"sass": "^1.26.10",
		"stylelint": "^13.6.1",
		"stylelint-config-prettier": "^8.0.2",
		"svelte": "^3.24.1",
		"svelte-preprocess": "^4.1.2",
		"tailwindcss": "^1.7.6"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
		}
	},
	"type": "module"
}
